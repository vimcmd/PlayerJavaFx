<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="hBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="50.0" minWidth="-Infinity" prefHeight="50.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sampleRefactored.controller.mediaControls.MediaControlsController">
   <children>
      <Button fx:id="slowPlayback" mnemonicParsing="false" text="Button" />
      <Button fx:id="playPauseBtn" mnemonicParsing="false" text="Button" />
      <Button fx:id="fastPlayback" mnemonicParsing="false" text="Button" />
      <Label fx:id="currentTimeLabel" text="Label" />
      <Slider fx:id="seekSlider" HBox.hgrow="ALWAYS" />
      <Label fx:id="totalTimeLabel" text="Label" />
      <Label fx:id="volumeLabel" text="?" />
      <Slider fx:id="volumeSlider" minWidth="30.0" prefWidth="70.0" value="100.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
